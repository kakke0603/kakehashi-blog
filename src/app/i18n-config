import i18next from "i18next";
i18next.init(
  {
    fallbackLng: "ja",
    ns: ["file1", "file2"],
    defaultNS: "file1",
    debug: true,
  },
  (err, t) => {
    if (err) return console.log("something went wrong loading", err);
    t(["i18n"] = { 
      locales: ["en", "ja"],
      defaultLocale: "ja"
  });
  }
);

document.getElementById('output').innerHTML = i18next.t('i18n');
